{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": false,
      "interface-name" : [true, "never-prefix"]
    },
    "rulesDirectory": [],
    "linterOptions": {
      "exclude": [
        "*.json",
        "**/*.json",
        "src/**/*.js",
        "src/**/*.jsx"
      ]
    }
}
