{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": false,
      "typescript/explicit-function-return-type": [
        "off",
        {
          "allowVoid": true
        }
      ]
    },
    "rulesDirectory": [],
    "linterOptions": {
      "exclude": [
        "*.json",
        "**/*.json"
      ]
    }
}
